package com.orienttech.statics.dao.entity.indexshows;

import static com.orienttech.statics.commons.utils.Contants.TJ_SCHEMA;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import com.orienttech.statics.commons.entity.BaseEntity;
import com.orienttech.statics.commons.utils.CommonHelper;

@Entity
@Table(name = "PRIV_FUNCTION_T", schema = TJ_SCHEMA)
public class PrivFunction extends BaseEntity{
private static final long serialVersionUID = 1963518584772322398L;
	
	@Id
	@Column(name="FUNCTION_ID")
	private int functionId;
	
	@Column(name="SYS_CODE",length=20)
	private String sysCode;
	
	@Column(name="NAME",length=200)
	private String name;
	
	@Column(name="LEVLES")
	private int levels;
	
	@Column(name="DETAIL_FLAG",length=1)
	private char detailFlag;
	
	@Column(name="ACTION",length=200)
	private String action;
	
	@Column(name="URL",length=800)
	private String url;
	
	@Column(name="ACTION_TYPE",length=10)
	private String actionType;
	
	@Column(name="IMG_NAME",length=200)
	private String imgName;
	
	@Column(name="HELP_NAME",length=200)
	private String helpName;
	
	@Column(name="ORG_PROPERTY",length=1)
	private char orgProperty;
	
	@Column(name="SUPER_ID",length=50)
	private int superId;
	
	@Column(name="STATE",length=1)
	private String state;
	
	@Column(name="INDEX_SHOWS",length=1000)
	private String indexShows;
	
	@Column(name="ANALYSIS_URL",length=800)
	private String analysisUrl;
	
	@Column(name="QUERY_URL",length=800)
	private String queryUrl;

	public int getFunctionId() {
		return functionId;
	}

	public void setFunctionId(int functionId) {
		this.functionId = functionId;
	}

	public String getSysCode() {
		return sysCode;
	}

	public void setSysCode(String sysCode) {
		this.sysCode = sysCode;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getLevels() {
		return levels;
	}

	public void setLevels(int levels) {
		this.levels = levels;
	}

	public char getDetailFlag() {
		return detailFlag;
	}

	public void setDetailFlag(char detailFlag) {
		this.detailFlag = detailFlag;
	}

	public String getAction() {
		return action;
	}

	public void setAction(String action) {
		this.action = action;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getActionType() {
		return actionType;
	}

	public void setActionType(String actionType) {
		this.actionType = actionType;
	}

	public String getImgName() {
		return imgName;
	}

	public void setImgName(String imgName) {
		this.imgName = imgName;
	}

	public String getHelpName() {
		return helpName;
	}

	public void setHelpName(String helpName) {
		this.helpName = helpName;
	}

	public char getOrgProperty() {
		return orgProperty;
	}

	public void setOrgProperty(char orgProperty) {
		this.orgProperty = orgProperty;
	}

	public int getSuperId() {
		return superId;
	}

	public void setSuperId(int superId) {
		this.superId = superId;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public String getIndexShows() {
		return indexShows;
	}

	public void setIndexShows(String indexShows) {
		this.indexShows = indexShows;
	}

	public String getAnalysisUrl() {
		return analysisUrl;
	}

	public void setAnalysisUrl(String analysisUrl) {
		this.analysisUrl = analysisUrl;
	}

	public String getQueryUrl() {
		return queryUrl;
	}

	public void setQueryUrl(String queryUrl) {
		this.queryUrl = queryUrl;
	}
	
	
}
