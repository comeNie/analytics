	private Long id;
	private Long pid;
	private String ssoId;
	private String userName;
	private boolean open;
	private boolean checked;
	
//	private List<TaskReceiverBo> children=new ArrayList<TaskReceiverBo>();
	
	
	public TaskReceiverBo() {
		super();
	}
	public TaskReceiverBo(Long pid, String ssoId, String userName) {
		super();
		this.pid = pid;
		this.ssoId = ssoId;
		this.userName = userName;
	}
	public String getSsoId() {
